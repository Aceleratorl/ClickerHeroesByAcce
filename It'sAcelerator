end
end)

local toggle = AutoRebirthSection:Toggle("EquipBestPets", false,"Toggle", function(ACC)
	_G.autocrystal = ACC
while _G.autocrystal == true do
	local args = {
    [1] = "Tap Capacity",
    [2] = "DamageCrystal",
    [3] = "Ninja",
    [4] = "1"
}

game:GetService("ReplicatedStorage").Shared.Modules.Utilities.NetworkUtility.Events.UpdateGemUpgrades:FireServer(unpack(args))
wait(0.1)
end
end)
